{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "types": ["node"]
  }, 
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
}
